{
    "extends": "tslint:recommended",
    "rules": {
        "max-line-length": {
            "options": [
                120
            ]
        },
        "interface-name": [
            true,
            "never-prefix"
        ],
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "object-literal-sort-keys": false,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false
    },
    "jsRules": {
        "max-line-length": {
            "options": [
                120
            ]
        }
    }
}